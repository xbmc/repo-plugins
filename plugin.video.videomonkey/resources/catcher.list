# data: *%s*
# header str|str
# limit: int
# action: str
# build: *%s*
# ext_target: RegEx
# ext_url: *%s*
# ext_data: *%s*
# ext_header str|str
# ext_limit: int
# ext_action: str
# ext_build: *%s*
