msgctxt "#30000"
msgid "General"
msgstr "Generale"

msgctxt "#30001"
msgid "Follow the instructions and enter PIN: "
msgstr "Segui le istruzioni e immetti il PIN: "

msgctxt "#30002"
msgid "Dropbox authorization required."
msgstr "E' richiesto un codice di autorizzazione di Dropbox."

msgctxt "#30003"
msgid "Go to: http://xbmc-dropbox.sourceforge.net"
msgstr "Vai a: http://xbmc-dropbox.sourceforge.net"

msgctxt "#30004"
msgid "Authorization finished. Account added:"
msgstr "Autorizzazione completata. Account aggiunto:"

msgctxt "#30005"
msgid "Clear Dropbox authorization code"
msgstr "Cancella il codice di autorizzazione di Dropbox"

msgctxt "#30006"
msgid "Dropbox authorization code"
msgstr "Codice di autorizzazione di Dropbox"

msgctxt "#30007"
msgid "Location for cached files"
msgstr "Posizione per i file in cache"

msgctxt "#30008"
msgid "View relevant files only"
msgstr "Vedi solo file importanti"

msgctxt "#30009"
msgid "30009"
msgstr "30009"

msgctxt "#30010"
msgid "Show me more..."
msgstr "Mostra di più..."

msgctxt "#30011"
msgid "Use passcode lock for account"
msgstr "Usare il blocco passcode per l'account"

msgctxt "#30012"
msgid "Change passcode..."
msgstr "Cambiare passcode..."

msgctxt "#30013"
msgid "Enter your passcode"
msgstr "Inserire il passcode"

msgctxt "#30014"
msgid "Wrong passcode!"
msgstr "Passcode sbagliato!"

msgctxt "#30015"
msgid "Passcode timeout (minutes)"
msgstr "Timeout del passoce (minuti)"

msgctxt "#30016"
msgid "Browse Dropbox"
msgstr "Esplora Dropbox"

msgctxt "#30017"
msgid "Search Dropbox"
msgstr "Cerca in Dropbox"

msgctxt "#30018"
msgid "Enter the search text"
msgstr "Inserire il testo da cercare"

msgctxt "#30019"
msgid "Search text has to be atleast 3 characters"
msgstr "Il testo da cercare deve essere almeno 3 caratteri"

msgctxt "#30020"
msgid "Searching in Dropbox for:"
msgstr "Sto cercando in Dropbox per:"

msgctxt "#30021"
msgid "Nothing found!"
msgstr "Non ho trovato niente!"

msgctxt "#30022"
msgid "Delete from Dropbox"
msgstr "Cancella da Dropbox"

msgctxt "#30023"
msgid "Do you want to delete this from Dropbox:"
msgstr "Si vuole cancellare questo da Dropbox:"

msgctxt "#30024"
msgid "Copy in Dropbox"
msgstr "Copia in Dropbox"

msgctxt "#30025"
msgid "Select the folder to "
msgstr "Selezionare la cartella per"

msgctxt "#30026"
msgid "copy to"
msgstr "copia in"

msgctxt "#30027"
msgid "Move in Dropbox"
msgstr "Sposta in Dropbox"

msgctxt "#30028"
msgid "move to"
msgstr "sposta in"

msgctxt "#30029"
msgid "Create new folder in Dropbox"
msgstr "Crea una nuova cartella in Dropbox"

msgctxt "#30030"
msgid "Enter the name of new folder"
msgstr "Inserire il nome della nuova cartella"

msgctxt "#30031"
msgid "Upload to Dropbox"
msgstr "Carica in Dropbox"

msgctxt "#30032"
msgid "Select the file to upload"
msgstr "Selezionare il file da caricare"

msgctxt "#30033"
msgid "Uploading file to Dropbox"
msgstr "Caricamento di file in Dropbox"

msgctxt "#30034"
msgid "Enter your new passcode"
msgstr "Inserire il nuovo passcode"

msgctxt "#30035"
msgid "Caching media files in background..."
msgstr "Creo la cache dei file in background"

msgctxt "#30036"
msgid "Stream video and music media (don't cache)"
msgstr "Fai lo stream di file video e musicali (non fare la cache)"

msgctxt "#30037"
msgid "Download from Dropbox"
msgstr "Scarica da Dropbox"

msgctxt "#30038"
msgid "download to"
msgstr "scarica in"

msgctxt "#30039"
msgid "Downloading file(s) from Dropbox"
msgstr "Sto scaricando file da Dropbox"

msgctxt "#30040"
msgid "Finished downloading file(s) to:"
msgstr "Completato lo scaricamento del/dei file in:"

msgctxt "#30041"
msgid "Downloading file:"
msgstr "Sto scaricando il/i file:"

msgctxt "#30042"
msgid "Add an account..."
msgstr "Aggiungere un account..."

msgctxt "#30043"
msgid "Finish adding account..."
msgstr "Completamento aggiunta account..."

msgctxt "#30044"
msgid "Remove account"
msgstr "Rimozione account"

msgctxt "#30045"
msgid "Do you want to remove the following account from Dbmc?"
msgstr "Vuoi rimuovere il seguente account da Dbmc?"

msgctxt "#30100"
msgid "Change synchronization..."
msgstr "Cambiare sincronizzazione..."

msgctxt "#30101"
msgid "Enable synchronization"
msgstr "Abilita sincronizzazione"

msgctxt "#30102"
msgid "Select the local folder"
msgstr "Selezione la cartella locale"

msgctxt "#30103"
msgid "Dbmc: Moved files/folders to new synchronization location:"
msgstr "Dbmc: Spostati file/cartella nella nuova posizione di sincronizzazione:"

msgctxt "#30104"
msgid "New synchronization location is not empty!"
msgstr "La posizione di sincronizzazione non è vuota!"

msgctxt "#30105"
msgid "Synchronization interval (minutes)"
msgstr "Intervallo di sincronizzazione (minuti)"

msgctxt "#30106"
msgid "Dbmc: Synchronization finished."
msgstr "Dbmc: sincronizzazione completata"

msgctxt "#30107"
msgid "Number of items synchronized: "
msgstr "Numero di oggetti da sincronizzare:"

msgctxt "#30108"
msgid "Synchronization locations"
msgstr "Posizioni per la sincronizzaizone"

msgctxt "#30109"
msgid "Select the Dropbox folder to synchronize with"
msgstr "Selezione la cartella di Dropbox da sincronizzare"

msgctxt "#30110"
msgid "Can't change settings while synchronizing!"
msgstr "Impossibile cambiare le impostazioni durante la sincronizzazione!"

msgctxt "#30111"
msgid "Can't enable synchronization:\nFirst set synchronization locations!"
msgstr "Impossibile abilitare la sincronizzazione:\nPrima impostare le posizioni di sincronizzazione!"

msgctxt "#30112"
msgid "Synchronize now"
msgstr "Sincronizza adesso"

msgctxt "#30113"
msgid "Stop Synchronizing now?"
msgstr "Fermo ora la sincronizzazione?"

msgctxt "#30114"
msgid "Dbmc: Synchronizing files..."
msgstr "Dbmc: sto sincronizzando i file..."

msgctxt "#30115"
msgid "Synchronization settings can't be changed here anymore!"
msgstr "Le impostazioni di sincronizzazione non possono più essere cambiate qui!"


# error messages
msgctxt "#30200"
msgid "Problem getting the access code!"
msgstr "Problema nell'ottenere il codice di accesso!"

msgctxt "#30201"
msgid "Authorization failed!"
msgstr "Autorizzazione fallita!"

msgctxt "#30202"
msgid "Did you copy the complete code?"
msgstr "Hai copiato il codice completo?"

msgctxt "#30203"
msgid "Action cannot be executed"
msgstr "30203"

msgctxt "#30204"
msgid "30204"
msgstr "30204"

msgctxt "#30205"
msgid "Dropbox authorization code failure!"
msgstr "Problema nel codice di autorizzazione di Dropbox!"

msgctxt "#30206"
msgid "Dropbox error occured!"
msgstr "Errore in Dropbox!"

msgctxt "#30207"
msgid "Please enter a valid passcode timeout (1 to 120 minutes)"
msgstr "Inserire un timeout per il passcode valido (da 1 a 120 minuti)"

msgctxt "#30208"
msgid "Please enter a valid synchronization interval (5 to 1440 minutes)"
msgstr "Inserire un intervallo di sincronizzazione valido (da 5 a 1440 minuti)"
