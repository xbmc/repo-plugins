{
    "name"              : "BiNGE",
    "plugin"            : "slyguy.binge",
    "priority"          : 100,
    "is_resolvable"     : "true",
    "assert"            : {
                            "play_movie":       ["title", "year"],
                            "play_episode":     ["showname", "season", "episode"],
                            "search_movie":     ["title"],
                            "search_episode":   ["showname"]
                          },
    "play_movie"        : [
                            "plugin://slyguy.binge/?_=search",
                            {"keyboard": "{title}"},
                            {"title": "{title}", "year": "{year}"}
                          ],
    "play_episode"      : [
                            "plugin://slyguy.binge/?_=search",
                            {"keyboard": "{showname}"},
                            {"title": "{showname}"},
                            {"title": "Season {season}"},
                            {"season": "{season}", "episode": "{episode}"}
                          ],
    "search_movie"      : [
                            "plugin://slyguy.binge/?_=search",
                            {"keyboard": "{title}"}
                          ],
    "search_episode"    : [
                            "plugin://slyguy.binge/?_=search",
                            {"keyboard": "{showname}"}
                          ]
}